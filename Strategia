class Latanie():
    def __init__(self):
        pass
    def ruch(self):
        print('Teraz lece')
class Bezruch():
    def __init__(self):
        pass
    def ruch(self):
        print('Nie ruszam sie')
class Labedz():
    def __init__(self):
        pass
    def strategia(self, obiekt):
        self.obiekt = obiekt
    def wykonanie(self):
        self.obiekt.ruch()
class Plywanie():
    def __init__(self):
        pass
    def ruch(self):
        print('Teraz plywam')
ptak = Labedz()
wybor = Latanie()
#strategia = Plywanie()
ptak.strategia(wybor)
ptak.wykonanie()
